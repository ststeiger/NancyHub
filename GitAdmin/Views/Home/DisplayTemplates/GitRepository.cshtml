@model GitManager.GitRepository
 
<li class="repo-list-item public fork">
    <div class="repo-list-stats">
        @Model.ProgrammingLanguage
        <a class="repo-list-stat-item tooltipped tooltipped-s" href="/ststeiger/WebGitNet/stargazers" aria-label="Stargazers">
            <span class="octicon octicon-star"></span>
            @Model.StarCount
        </a>
        <a class="repo-list-stat-item tooltipped tooltipped-s" href="/ststeiger/WebGitNet/network" aria-label="Forks">
            <span class="octicon octicon-git-branch"></span>
            @Model.ForkCount
        </a>
    </div>

    <h3 class="repo-list-name">
        <a href="/ststeiger/WebGitNet">
            @Model.Name
        </a>
    </h3>

    <p class="repo-list-info">


    @if(Model.HasForkSource)
    {
    	<span class="octicon octicon-repo-forked"></span>
    	@:forked from 
    	<a href="@Model.ForkSource">@Model.ForkSource</a>
    }


	<span class="octicon octicon-repo-forked"></span>


        
    </p>

    <p class="repo-list-description">
        @Model.Description
    </p>

    <p class="repo-list-meta">
        Updated @Model.LastModifiedString
        <!--
        Updated <time datetime="2015-04-17T09:50:17Z" is="relative-time" title="Apr 17, 2015, 11:50 AM GMT+2">2 days ago</time>
            -->
    </p>

    <!--
    <div class="participation-graph">
        <svg class="bars" width="640" height="80">
            <g transform="translate(627.81, 0)">
                <rect width="11.31" height="4.0" y="76.0" fill="#f8f8f8"></rect>
                <rect width="11.31" height="4.0" y="76.0" fill="#dedede"></rect>
            </g>
        </svg>

    </div>
    -->
</li>
